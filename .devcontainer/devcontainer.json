{
    "name": "Node12+Serverless",
	"build": {
		"dockerfile": "Dockerfile"
    },
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
    },
    "forwardPorts": [5000, 5001, 7071],        
    "extensions": [
        "coenraads.bracket-pair-colorizer-2",
        "donjayamanne.githistory",
